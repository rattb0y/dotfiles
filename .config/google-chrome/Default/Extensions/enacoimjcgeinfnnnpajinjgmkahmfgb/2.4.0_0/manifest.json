{
   "app": {
      "background": {
         "persistent": false,
         "scripts": [ "js/eventPage.js" ]
      }
   },
   "author": "Cleanflight Project",
   "default_locale": "en",
   "description": "Crossplatform configuration tool for Cleanflight flight control system",
   "icons": {
      "128": "images/cf/cf_icon_128.png"
   },
   "key": "",
   "manifest_version": 2,
   "minimum_chrome_version": "38",
   "name": "Cleanflight - Configurator",
   "offline_enabled": true,
   "permissions": [ "https://www.google-analytics.com/", "https://maps.googleapis.com/*", "https://*.github.com/", "https://*.githubusercontent.com/", "http://*.cleanflight.com/", "https://*.amazonaws.com/", "https://cleanflight.slack.com/*", "serial", "usb", "storage", "fileSystem", "fileSystem.write", "fileSystem.retainEntries", "notifications", "alwaysOnTopWindows", {
      "usbDevices": [ {
         "productId": 57105,
         "vendorId": 1155
      } ]
   }, "webview", "unlimitedStorage" ],
   "short_name": "Cleanflight",
   "sockets": {
      "tcp": {
         "connect": "*:*"
      }
   },
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2.4.0",
   "webview": {
      "partitions": [ {
         "accessible_resources": [ "tabs/map.html", "js/tabs/map.js", "/images/icons/cf_icon_position.png" ],
         "name": "map"
      } ]
   }
}
